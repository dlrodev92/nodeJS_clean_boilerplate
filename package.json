  "description": "A boilerplate node application that follows the principles of clean Architecture. Ts.",
  "main": "index.js",
"scripts": {
  "setup": "node setup.js",
  "postinstall": "npx prisma generate && npx prisma migrate dev --name init",
  "dev": "nodemon",
  "build": "tsc",
  "start": "node dist/app.js",
  "migrate:dev": "npx prisma migrate dev --name init",
  "migrate:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
  "prettier": "npx prettier --write .",
  "test": "jest",
  "test:watch": "jest --watch",
  "test:coverage": "jest --coverage"
},
